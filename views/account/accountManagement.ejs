<% if (title) { %>
  <h1>
    <%= title %>
  </h1>
  <% } else { res.redirect('/') } %>

    <%- messages() %>

      <h2>Welcome <%= locals.accountData.account_firstname %>
      </h2>
      <p>You're logged in.</p>
      <a title="Update Account Information" href="/account/updateAccount/<%= locals.accountData.account_id %>">Update
        Account Information</a>

      <% if (locals.accountData.account_type==="Employee" || locals.accountData.account_type==="Admin" ) { %>
        <h3>Inventory Management</h3>
        <a title="Inventory Management" href="/inv/">Manage Inventory</a>
        <% } %>